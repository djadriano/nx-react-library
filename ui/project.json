{
  "name": "ui",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "ui/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project ui --web",
  "targets": {
    "build": {
      "outputs": ["dist/ui"]
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/ui"
      }
    },
    "release": {
